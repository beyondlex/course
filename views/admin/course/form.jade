	script(src="/components/jquery-file-upload/js/jquery.fileupload.js")

	script.
		$(function(){
			$('#fileupload').fileupload({
				dataType: 'json',
				done: function(e, data) {
					console.log(e);
					console.log(data);
				},	
			})	
		})


		
